{
  "name": "bowling-game",
  "version": "0.1.0",
  "repository": "https://github.com/austinhung81/bowling-game",
  "homepage": "https://austinhung81.github.io/bowling-game/",
  "description": "bowling game",
  "scripts": {
    "format": "prettier --check \"**/*.{css,js,json,md,ts,yml,yaml}\"",
    "format:fix": "prettier --write \"**/*.{css,js,json,md,ts,yml,yaml}\"",
    "husky:lint-staged": "lint-staged",
    "husky:pre-commit": "run-p husky:lint-staged",
    "lint": "run-p husky:pre-commit",
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "prebuild": "rm -rf dist",
    "build": "npm run test && webpack-cli --config ./webpack.prod.js",
    "test": "jest",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-icons": "^3.11.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^25.1.2",
    "@types/react-test-renderer": "^17.0.0",
    "@types/webpack-env": "^1.15.1",
    "autoprefixer": "^9.7.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.21.5",
    "fetch-mock": "^9.11.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-style-loader": "^2.3.0",
    "react-test-renderer": "^16.14.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": "12",
    "npm": ">=6.13.4"
  },
  "files": [
    "src"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run husky:pre-commit"
    }
  },
  "license": "UNLICENSED",
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ],
    "*.{css,js,json,md,ts,yaml,yml}": [
      "prettier --write"
    ]
  }
}
